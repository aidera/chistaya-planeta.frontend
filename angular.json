{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "chistaya-planeta": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/chistaya-planeta",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/android-icon-36x36.png",
              "src/android-icon-48x48.png",
              "src/android-icon-72x72.png",
              "src/android-icon-96x96.png",
              "src/android-icon-144x144.png",
              "src/android-icon-192x192.png",
              "src/apple-icon.png",
              "src/apple-icon-57x57.png",
              "src/apple-icon-60x60.png",
              "src/apple-icon-72x72.png",
              "src/apple-icon-76x76.png",
              "src/apple-icon-114x114.png",
              "src/apple-icon-120x120.png",
              "src/apple-icon-144x144.png",
              "src/apple-icon-152x152.png",
              "src/apple-icon-precomposed.png",
              "src/browserconfig.xml",
              "src/favicon-16x16",
              "src/favicon-32x32",
              "src/favicon-96x96",
              "src/manifest.json",
              "src/ms-icon-70x70.png",
              "src/ms-icon-144x144.png",
              "src/ms-icon-150x150.png",
              "src/ms-icon-310x310.png"
            ],
            "styles": ["src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "socket.io-client",
              "socket.io-parser",
              "debug"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "chistaya-planeta:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "chistaya-planeta:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "chistaya-planeta:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/android-icon-36x36.png",
              "src/android-icon-48x48.png",
              "src/android-icon-72x72.png",
              "src/android-icon-96x96.png",
              "src/android-icon-144x144.png",
              "src/android-icon-192x192.png",
              "src/apple-icon.png",
              "src/apple-icon-57x57.png",
              "src/apple-icon-60x60.png",
              "src/apple-icon-72x72.png",
              "src/apple-icon-76x76.png",
              "src/apple-icon-114x114.png",
              "src/apple-icon-120x120.png",
              "src/apple-icon-144x144.png",
              "src/apple-icon-152x152.png",
              "src/apple-icon-precomposed.png",
              "src/browserconfig.xml",
              "src/favicon-16x16",
              "src/favicon-32x32",
              "src/favicon-96x96",
              "src/manifest.json",
              "src/ms-icon-70x70.png",
              "src/ms-icon-144x144.png",
              "src/ms-icon-150x150.png",
              "src/ms-icon-310x310.png"
            ],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "chistaya-planeta:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "chistaya-planeta:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "chistaya-planeta",
  "cli": {
    "analytics": "b515b9bd-d944-4098-bc90-a0d82adb0500"
  }
}
